{
  "name": "careerpulse-backend",
  "version": "1.0.0",
  "description": "Backend API for CareerPulse job application tracker",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "node --watch server.js",
    "test": "vitest run",
    "test:unit": "vitest run tests/unit/",
    "test:properties": "vitest run tests/properties/",
    "test:integration": "vitest run tests/integration/",
    "test:e2e": "vitest run tests/e2e/",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "test:gemini": "node test-gemini.js",
    "test:oauth": "node test-oauth-url.js",
    "test:all": "npm run test:gemini && npm run test:oauth && npm test",
    "setup-secrets": "node scripts/setup-secrets.js",
    "validate": "npm run test:gemini && npm run test:oauth && echo 'âœ… All validations passed!'"
  },
  "keywords": [
    "job-tracker",
    "gmail",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/secret-manager": "^6.1.1",
    "@google/generative-ai": "^0.24.1",
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "express-session": "^1.17.3",
    "googleapis": "^128.0.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.3",
    "morgan": "^1.10.1",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^4.0.17",
    "fast-check": "^4.5.3",
    "supertest": "^7.2.2",
    "uuid": "^13.0.0",
    "vitest": "^4.0.17"
  }
}
